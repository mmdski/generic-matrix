{
    "files.associations": {
        "*.h": "c",
        "chrono": "c",
        "ios": "c",
        "ostream": "cpp"
    },
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.codeAnalysis.exclude": {
        "build/_deps/googletest-*/": true
    },
    "editor.tabSize": 2,
    "cmake.generator": "Ninja",
    "cmake.launchBehavior": "reuseTerminal",
    "cmake.revealLog": "never",
    "cmake.configureSettings": {
        "TESTS": 1
    },
    "cSpell.words": [
        "GMATRIX"
    ]
}
